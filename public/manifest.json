{
    "manifest_version": 3,
    "name": "Extension Name",
    "version": "0.1",
    "description": "Some Description",
    "permissions": [
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [{
        "js": [ "static/js/content.js" ],
        "matches": [
            "http://*.example.com/*", "https://*.example.com/*"
        ]
    }],
    "icons": {
        "192": "logo192.png"
    },
    "action": {
        "default_icon": {
            "192": "logo192.png"
        },
        "default_title": "Popup",
        "default_popup": "popup.html"
    },
    "options_page": "options.html"
}
